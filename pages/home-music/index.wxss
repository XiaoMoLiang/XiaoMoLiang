/* pages/home-music/index.wxss */
page{
    padding:  0 20rpx;
    box-sizing: border-box;
}
.van-search{
    padding: 10px 0 !important;
}
.van-search_content{
    background-color: #fff !important ;
}

/* 轮播图样式 */
.swiper{
    border-radius: 10rpx;
    overflow: hidden;
    transform: translateY(0);
}

.swiper .swiper-item{
    display: flex;
}
.swiper .swiper-item .image{
    width: 100%;
}